{
  "name": "yamde",
  "version": "1.0.0",
  "description": "Yet another markdown editor.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "test": "jest --config jestconfig.json",
    "prepublishOnly": "npm test && npm run lint",
    "build": "tsc"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AdrianApan/yamde.git"
  },
  "keywords": [
    "markdown",
    "editor",
    "wysiwyg"
  ],
  "author": "Adrian Apan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AdrianApan/yamde/issues"
  },
  "homepage": "https://github.com/AdrianApan/yamde",
  "devDependencies": {
    "@types/jest": "^26.0.22",
    "@types/react": "^17.0.3",
    "@types/showdown": "^1.9.3",
    "jest": "^26.6.3",
    "prettier": "^2.2.1",
    "ts-jest": "^26.5.4",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.2.4",
    "typescript-plugin-css-modules": "^3.2.0"
  },
  "dependencies": {
    "react": "^16.14.0",
    "react-icons": "^4.2.0",
    "react-jss": "^10.6.0",
    "showdown": "^1.9.1"
  }
}
